    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Debug Text",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}",
            "program": ".\\node_modules\\jsdoc\\jsdoc.js",
            "args": [ "tmp\\word.js", "-t", ".", "-c", ".\\conf.json" ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Debug Spreadsheet",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}",
            "program": ".\\node_modules\\jsdoc\\jsdoc.js",
            "args": [ "tmp\\cell.js", "-t", ".", "-c", ".\\conf.json" ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Debug Presentation",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}",
            "program": ".\\node_modules\\jsdoc\\jsdoc.js",
            "args": [ "tmp\\slide.js", "-t", ".", "-c", ".\\conf.json" ]
        }
    ]
}