{
  "name": "@onlyoffice/eslint-presentation",
  "private": true,
  "type": "module",
  "main": "lib/main.ts",
  "scripts": {
    "test:types": "tsc",
    "test:unit": "c8 --all --src lib --reporter html --reporter text --check-coverage --branches 80 --functions 80 --lines 80 --statements 80 --per-file tsx node_modules/uvu/bin.js lib ^.*\\.test\\.ts$",
    "test": "pnpm test:types && pnpm test:unit"
  },
  "dependencies": {
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin-js": "^2.1.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.12.12",
    "eslint": "^9.3.0",
    "eslint-plugin-jsonc": "^2.15.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "c8": "^9.1.0",
    "tsx": "^4.10.5",
    "uvu": "^0.5.6"
  }
}
