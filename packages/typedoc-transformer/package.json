{
  "name": "@onlyoffice/typedoc-transformer",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./dist/main.js",
    "./console.js": "./dist/console.js"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test:types": "tsc --noEmit",
    "test:unit": "c8 --config ../../c8.config.json tsx node_modules/uvu/bin.js lib ^.*\\.test\\.ts$",
    "test": "pnpm test:types && pnpm test:unit"
  },
  "dependencies": {
    "@onlyoffice/async-transform": "workspace:^",
    "@onlyoffice/console": "workspace:^",
    "@onlyoffice/errors": "workspace:^",
    "@onlyoffice/library-declaration": "workspace:^",
    "@onlyoffice/mdast-util-eslint": "workspace:^",
    "@onlyoffice/mdast-util-first-sentence": "workspace:^",
    "@onlyoffice/result": "workspace:^",
    "@onlyoffice/strings": "workspace:^",
    "@onlyoffice/typedoc-transport": "workspace:^",
    "@onlyoffice/typedoc-util-is-reflection": "workspace:^",
    "@onlyoffice/typedoc-util-is-type": "workspace:^",
    "@types/node": "20.12.12",
    "@types/remark-strip-html": "workspace:^",
    "mdast-util-from-markdown": "2.0.0",
    "mdast-util-to-markdown": "2.1.0",
    "remark-strip-html": "1.0.2",
    "typedoc": "0.26.11"
  },
  "devDependencies": {
    "c8": "9.1.0",
    "tsx": "4.16.5",
    "typescript": "5.7.2",
    "uvu": "0.5.6"
  }
}
