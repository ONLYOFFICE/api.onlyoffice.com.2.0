{
  "name": "@onlyoffice/server-client",
  "type": "module",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "demo:server": "store-demo",
    "playwright": "cd node_modules/@web/test-runner-playwright/node_modules/.bin && ./playwright",
    "test:all": "pnpm test:types && pnpm test:unit && pnpm test:e2e",
    "test:e2e": "tsx node_modules/@web/test-runner/dist/bin.js --config wtr.config.ts",
    "test:types": "tsc --noEmit",
    "test:unit": "c8 --config ../../c8.config.json --exclude main.ts --exclude main.e2e.ts tsx node_modules/uvu/bin.js lib ^.*\\.unit\\.ts$",
    "test": "tsx test.ts"
  },
  "dependencies": {
    "@onlyoffice/document-server-types": "workspace:^"
  },
  "devDependencies": {
    "@onlyoffice/node-http": "workspace:^",
    "@onlyoffice/store-demo": "workspace:^",
    "@types/mocha": "10.0.6",
    "@types/node": "20.12.12",
    "@web/dev-server": "0.4.4",
    "@web/dev-server-esbuild": "1.0.2",
    "@web/test-runner": "0.18.2",
    "@web/test-runner-playwright": "0.11.0",
    "c8": "9.1.0",
    "tsx": "4.16.5",
    "typescript": "5.7.2",
    "uvu": "0.5.6"
  }
}
