{
  "name": "@onlyoffice/hastscript",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./dist/main.js",
    "./builder.js": "./dist/builder.js",
    "./context.js": "./dist/context.js",
    "./elements.js": "./dist/elements.js",
    "./texts.js": "./dist/texts.js"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test:types": "tsc --noEmit",
    "test:unit": "c8 --config ../../c8.config.json tsx node_modules/uvu/bin.js lib ^.*\\.test\\.ts$",
    "test": "pnpm test:types && pnpm test:unit"
  },
  "dependencies": {
    "@types/hast": "3.0.4",
    "@types/unist": "3.0.3",
    "hastscript": "8.0.0",
    "preact": "10.22.0",
    "unist-builder": "4.0.0"
  },
  "devDependencies": {
    "c8": "9.1.0",
    "tsx": "4.16.5",
    "typescript": "5.7.2",
    "uvu": "0.5.6"
  }
}
