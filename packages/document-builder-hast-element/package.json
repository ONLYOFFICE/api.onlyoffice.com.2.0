{
  "name": "@onlyoffice/document-builder-hast-element",
  "type": "module",
  "main": "lib/main.ts",
  "scripts": {
    "clean": "rm --recursive dist",
    "pretest": "pnpm run clean && esbuild --bundle lib/main.ts --format=esm --keep-names --outfile=dist/main.js && esbuild lib/main.test.ts --format=esm --keep-names --outfile=dist/main.test.js",
    "test": "c8 --reporter html --reporter text uvu dist ^.*\\.test\\.js$"
  },
  "dependencies": {
    "@onlyoffice/document-builder-html-element": "workspace:^",
    "@onlyoffice/document-editor-hast-element": "workspace:^"
  },
  "devDependencies": {
    "@onlyoffice/document-editor-html-element": "workspace:^",
    "@types/hast": "^3.0.4",
    "c8": "^9.1.0",
    "esbuild": "^0.21.2",
    "hast-util-to-html": "^9.0.1",
    "typescript": "^5.4.5",
    "uvu": "^0.5.6"
  },
  "engines": {
    "node": "^21"
  }
}
