{
  "name": "@onlyoffice/jq",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./dist/main.js",
    "./next.js": "./dist/next.js"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test:types": "tsc --noEmit",
