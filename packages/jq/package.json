  "dependencies": {
    "@onlyoffice/command": "workspace:^",
    "@onlyoffice/stream-string": "workspace:^",
    "@types/node": "20.12.12"
  },
  "devDependencies": {
    "c8": "9.1.0",
    "tsx": "4.16.5",
    "typescript": "5.7.2",
    "uvu": "0.5.6"
  }
}
