{
  "name": "@onlyoffice/storybook",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "storybook build --config-dir ./lib --disable-telemetry --output-dir ./dist",
    "preview": "eleventy-dev-server --dir dist",
    "serve": "storybook dev --config-dir ./lib --no-open --port 6006"
  },
  "devDependencies": {
    "@11ty/eleventy-dev-server": "1.0.4",
    "@onlyoffice/site-kit": "workspace:^",
    "@onlyoffice/ui-kit": "workspace:^",
    "@storybook/addon-essentials": "8.0.0",
    "@storybook/blocks": "8.0.0",
    "@storybook/preact": "8.0.0",
    "@storybook/preact-vite": "8.0.0",
    "browserslist": "4.23.2",
    "lightningcss": "1.23.0",
    "preact": "10.22.0",
    "preact-render-to-string": "6.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "8.0.0",
    "typescript": "5.4.5",
    "vite": "5.1.6"
  }
}
