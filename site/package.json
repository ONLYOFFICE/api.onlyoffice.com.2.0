{
  "name": "@onlyoffice/documentation-site",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "DOCUMENTATION_RUN_MODE=build node --require esbuild-register --max-old-space-size=8192 ./node_modules/@11ty/eleventy/cmd.js --config ./eleventy.config.ts",
    "preview": "DOCUMENTATION_RUN_MODE=preview node --require esbuild-register --max-old-space-size=8192 ./node_modules/@11ty/eleventy/cmd.js --config ./eleventy.config.ts",
    "serve": "DOCUMENTATION_RUN_MODE=serve node --require esbuild-register --max-old-space-size=8192 ./node_modules/@11ty/eleventy/cmd.js --serve --quiet --config ./eleventy.config.ts"
  },
